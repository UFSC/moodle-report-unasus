<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="report/unasus/db" VERSION="20240613" COMMENT="XMLDB file for Moodle report/unasus"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="report_unasus" COMMENT="Default comment for report_unasus, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="report_unasus_consolidado" COMMENT="Dados consolidados para a construção dos relatórios">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="user_id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="grade" TYPE="number" LENGTH="10" NOTNULL="false" SEQUENCE="false" DECIMALS="5" COMMENT="Nota obtida na atividade"/>
        <FIELD NAME="grademax" TYPE="number" LENGTH="10" NOTNULL="true" DEFAULT="100.00000" SEQUENCE="false" DECIMALS="5" COMMENT="Nota máxima possível de se obter na atividade"/>
        <FIELD NAME="submission_date" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Data de entrega da atividade"/>
        <FIELD NAME="submission_modified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Data de modificação da entrega da atividade"/>
        <FIELD NAME="grade_created" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Data de avaliação da entrega da atividade"/>
        <FIELD NAME="grade_modified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Data de modificação da avaliação da entrega da atividade"/>
        <FIELD NAME="status" TYPE="char" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Status da atividade"/>
        <FIELD NAME="activity_type" TYPE="char" LENGTH="1" NOTNULL="false" SEQUENCE="false" COMMENT="Tipo da atividade (A|Q|F|D|S|L)"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>